{
  "name": "react-simple-boilerplate",
  "version": "1.0.0",
  "description": "Simple and lightweight Boilerplate for ReactJS projects",
  "main": "server.js",
  "scripts": {
    "lint": "eslint src",
    "start": "node server.js",
    "test": "npm run scss-compile",
    "scss-compile": "node-sass -rw styles -o assets/css",
    "watch": "npm run scss-compile",
    "build": "bable src -d dist"
  },
  "repository": "src",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "reload",
    "live",
    "edit",
    "webpack",
    "node-scss"
  ],
  "author": "Ranjan De",
  "license": "SIT",
  "bugs": {
    "url": "https://github.com/noahlotzer/react-simple-boilerplate/issues"
  },
  "homepage": "http://localhost:3000",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^20.0.4",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "babel-loader": "^7.0.0",
    "d3-array": "^1.2.0",
    "d3-scale": "^1.0.6",
    "d3-time-format": "^2.0.5",
    "flux": "^3.1.2",
    "history": "^4.6.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-easy-chart": "^0.1.13",
    "react-faux-dom": "^4.0.3",
    "react-fontawesome": "^1.6.1",
    "react-google-chart": "^0.1.2",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-virtual-dom": "^0.9.3"
  }
